<script type="text/javascript">
jQuery.noConflict();
jQuery(document).ready(function($){
	
	 jQuery(window).scroll(function () { 
		if(jQuery(window).scrollTop() > jQuery(".smartBannerIdentifier").offset().top)
		{
			jQuery(".top-menu-container").css("position", "fixed");
			jQuery(".top-menu-container").css("top", "0");
			jQuery(".top-menu-container").css("left", "0");
			jQuery(".top-menu-container").css("width", "100%");
		}	
		else{
			jQuery(".top-menu-container").css("position", "relative");
			//jQuery(".top-menu-container").css("width", "100%");
			jQuery(".top-menu-container").css("top", "0");
		}
		
    });
});
</script>
