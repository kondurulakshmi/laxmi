 function hideMenu() {
 if (timeout)
 clearTimeout(timeout);
 timeout = setTimeout(function() {
 timeout = null;
 $('#block-cart').slideUp();
 }, 200);
 }

 function showMenu() {
 $('#top-links').css('display','none');
